<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" type="text/css" href="/static/style.css">
    <meta charset="utf-8">
    <title><%= title %></title>
</head>

<body>
    <h1><%= h1 %></h1>
    <ul class="alcoholList">
        <li>All</li>
        <% alcoholList.forEach((item) => { %>
        <li><%= item %></li>
        <% }) %>
    </ul>
    <div class="container">
        <ul class="cocktailList">
            <% cocktails.forEach((item) => { %>
            <li><%= item.name %></li>
            <li><%= item.contains %></li>
            <% }) %>
        </ul>
        <ul class="price">
            <% cocktails.forEach((item) => { %>
            <li><%= item.price %></li>
            <% }) %>
        </ul>
    </div>
</body>

</html>